{
  "name": "proyecto-frontend-mrcinema",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:container": "ng serve container -o --port 8080 --configuration=local",
    "start:marketing": "ng serve marketing -o --port 8081 --configuration=local",
    "start:backoffice": "cd projects/backoffice && npm run start",
    "start:all": "concurrently \"npm run start:container\" \"npm run start:marketing\" \"npm run start:backoffice\" ",
    "start:test:container": "ng serve container --configuration=test",
    "start:test:marketing": "ng serve marketing --configuration=test",
    "start:test:backoffice": "cd projects/backoffice && npm run start:test",
    "start:test:all": "concurrently \"npm run start:test:container\" \"npm run start:test:marketing\" \"npm run start:test:backoffice\" ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^16.0.4",
    "@angular-architects/module-federation-tools": "^16.0.4",
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "~16.1.5",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "~16.1.5",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "angular-feather": "^6.5.0",
    "ngx-build-plus": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.6",
    "@angular/cli": "~16.0.3",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "concurrently": "^8.2.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.0.0",
    "typescript": "~5.0.2"
  }
}
